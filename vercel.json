{
  "version": 2,
  "builds": [{ "src": "index.html", "use": "@vercel/static" }, { "src": "mic-test.html", "use": "@vercel/static" }],
  "routes": [{ "src": "/(.*)", "dest": "/index.html" }],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Permissions-Policy", "value": "microphone=(self)" }
      ]
    }
  ]
}
